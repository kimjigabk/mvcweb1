@model IEnumerable<WebApplication1.Models.Music>

@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Add Music", "Add", "Musics", null, new { @class = "btn btn-primary" })
</p>

<p>@Model.Single(m=>m.Id==1).YoutubeUrl</p>
<table id="musics" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Title</th>
            <th>Artist</th>
            <th>View</th>
        </tr>
    </thead>
</table>

@section scripts
{

        <script src="~/scripts/inserttable.js"></script>


}